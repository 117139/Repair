.container{
	min-height: 100vh;
	background: #eeeeee;
	border-top: 1px solid #eee;
	padding-top: 100rpx;
}
.olist_tit{
  border-top: 1px solid #eee;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: stretch;
  background-color: #fff;
}
.olist_tit view{
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 3rpx solid transparent;
}
.olist_tit .typecur{
  border-bottom: 3rpx solid #3ea5ff;
	color: #3ea5ff;
}
.orderOne{
	margin-top: 18rpx;
	width: 100%;
	height: 160rpx;
	padding: 38rpx 24rpx;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background-color: #fff;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
}
